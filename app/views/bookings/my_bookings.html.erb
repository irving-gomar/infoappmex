<h1>Mis reservas</h1>
<% @bookings.each do |booking| %>
  <div>
    <p>Booking: <%= booking.id %></p> 
    <p>ONG: <%= booking.bed.shelter.ong.name %></p>
    <p>Shelter: <%= booking.bed.shelter.name %></p>
    <%= link_to shelter_path(booking.bed.shelter), class: 'btn btn-info' do %>
      Ver Refugio
    <% end %>
  </div>
<% end %>