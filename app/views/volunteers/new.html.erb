<%= simple_form_for [@ong, @service] do |f|%>
  <%= f.input :name, :collection => ["Asistencia Medica", "Asistencia Alimentaria", "Proteccion y Ayuda Legal", "Albergue"] %>
  <%= f.input :date_from, html5: true %>
  <%= f.input :date_to, html5: true %>
  <%= f.input :description %>
  <%= f.button :submit %>
<% end %>

